{
  "name": "uaproger/aprog",
  "description": "Бібліотека для роботи з властивостями, та інші хелпери",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "files": [
      "src/Helpers/Helper.php",
      "src/Helpers/Lang.php",
      "src/Helpers/Logger.php",
      "src/Helpers/Timer.php"
    ],
    "psr-4": {
      "Aprog\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Oleksandr Zadorozhnii",
      "email": "developer@alexproger.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aprog\\AprogServiceProvider"
      ]
    }
  },
  "require-dev": {
    "larapack/dd": "1.1"
  }
}
